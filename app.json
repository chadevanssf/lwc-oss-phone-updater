{
    "name": "lwc-oss-phone-updater",
    "description": "Phone Updater for Heroku Connect with LWC OSS",
    "repository": "https://github.com/chadevanssf/lwc-oss-phone-updater",
    "keywords": ["LWC", "LWC OSS"],
    "scripts": {},
    "formation": {
        "web": {
            "quantity": 1
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/nodejs"
        }
    ],
    "env": {
        "NODE_ENV": {
            "description": "Node.js variable that determines if this is production or testing, one of 'production', 'test', or 'development'",
            "value": "development"
        }
    },
    "environments": {
        "test": {
            "env": {
                "NODE_ENV": "test"
            },
            "scripts": {
                "test": "yarn test:unit"
            }
        }
    }
}
